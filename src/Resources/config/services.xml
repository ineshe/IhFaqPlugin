<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="IhFaqPlugin\Core\Content\Faq\FaqDefinition">
            <tag name="shopware.entity.definition" entity="ih_faq" />
        </service>

        <service id="IhFaqPlugin\Extension\Content\Product\ProductExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="IhFaqPlugin\Extension\Content\Product\ProductExtensionDefinition">
            <tag name="shopware.entity.definition" entity="ih_product_extension" />
        </service>
    </services>
</container>